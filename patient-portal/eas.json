{
  "cli": {
    "version": ">= 12.0.0"
  },
  "build": {
    "preview": {
      "ios": {
        "distribution": "internal",
        "simulator": false,
        "autoIncrement": "version",
        "buildConfiguration": "Release"
      },
      "android": {
        "gradleCommand": ":app:assembleRelease"
      },
      "env": {
        "EXPO_PUBLIC_API_BASE": "https://patient-portal-admin-service-production.up.railway.app/api",
        "EXPO_PUBLIC_SOCKET_URL": "https://patient-portal-admin-service-production.up.railway.app"
      }
    },
    "production": {
      "ios": {
        "distribution": "store",
        "autoIncrement": "version"
      },
      "android": {
        "gradleCommand": ":app:assembleRelease"
      },
      "env": {
        "EXPO_PUBLIC_API_BASE": "https://patient-portal-admin-service-production.up.railway.app/api",
        "EXPO_PUBLIC_SOCKET_URL": "https://patient-portal-admin-service-production.up.railway.app"
      }
    }
  },
  "submit": {
    "production": {
      "ios": {
        "ascAppId": null
      },
      "android": {
        "serviceAccountKeyPath": null,
        "track": "internal"
      }
    }
  }
}
